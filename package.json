    "graphql-sql-bridge": "https://github.com/sywka/graphql-sql-bridge.git#v.0.0.3",
    "graphql-voyager": "^1.0.0-rc.15",
    "morgan": "^1.9.0",
    "nesthydrationjs": "^1.0.3",
    "path": "^0.12.7",
    "pm2": "^2.10.1",
    "pug": "^2.0.0-rc.4",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/config": "^0.0.34",
